client
dev tun
proto udp
remote c2cpc3.camptocamp.com 1194
resolv-retry infinite
nobind
persist-key
persist-tun
ca /etc/openvpn/camptocamp/ca.crt
auth-user-pass
comp-lzo
verb 3
tls-auth /etc/openvpn/camptocamp/ta.key 1
script-security 2
up /etc/openvpn/scripts/up.sh
down /etc/openvpn/scripts/down.sh
